<Window x:Class="CHET_WPF.LogsFromInteraction"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:CHET_WPF"
      mc:Ignorable="d" 
      Title="LogsFromInteraction"  Height="450" Width="800"
        MinHeight="450"
        MinWidth="800"
        MaxHeight="450"
        MaxWidth="800">

    <Grid>
        <Button Content="See Logs" HorizontalAlignment="Left" Margin="10,5,0,0" VerticalAlignment="Top" Click="Button_Click"/>
        <ScrollViewer x:Name="Log"  Margin="0,30,10,0" Content=""/>
        <Button x:Name="ButtonHome" HorizontalAlignment="Left" Height="43" Margin="698,362,0,0" VerticalAlignment="Top" Width="63" Click="ButtonHome_Click">
            <Button.Background>
                <ImageBrush ImageSource="/button_home.png"/>
            </Button.Background>

        </Button>


    </Grid>
</Window>
