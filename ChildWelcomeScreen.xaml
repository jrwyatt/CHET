<Window x:Class="CHET_WPF.ChildWelcomeScreen"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:CHET_WPF"
      mc:Ignorable="d" 
      Title="ChildWelcomeScreen" Height="450" Width="800" 
        MinHeight="450"
        MinWidth="800"
        MaxHeight="450"
        MaxWidth="800">

    <Grid>

            <Frame x:Name="main"  NavigationUIVisibility="Hidden" Background="#FFF3F8BB"/>
  
        <Button x:Name="ButtonHome" HorizontalAlignment="Left" Height="43" Margin="680,352,0,0" VerticalAlignment="Top" Width="63" Click="ButtonHome_Click">
            <Button.Background>
                <ImageBrush ImageSource="/button_home.png"/>
            </Button.Background>

        </Button>

    </Grid>
</Window>
